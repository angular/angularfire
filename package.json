    "reflect-metadata": "0.1.2",
    "rimraf": "^2.5.4",
    "rollup": "^0.64.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-watch": "^4.3.1",
    "schematics-utilities": "^1.1.1",
    "shelljs": "^0.8.0",
