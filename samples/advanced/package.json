{
  "name": "sample",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cd ../.emulator && firebase emulators:exec --import seed \"cd ../advanced && ng serve --hmr\"",
    "build": "ng build --stats-json",
    "build:prod": "ng build --configuration production --stats-json",
    "analyze": "webpack-bundle-analyzer dist/sample/browser/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "dev:ssr": "cd ../.emulator && firebase emulators:exec --import seed \"cd ../advanced && ng run sample:serve-ssr\"",
    "dev:ssr:zoneless": "cd ../.emulator && firebase emulators:exec --import seed \"cd ../advanced && ng run sample:serve-ssr:zoneless\"",
    "serve:ssr": "node dist/sample/server/main.js",
    "build:ssr": "ng build && ng run sample:server",
    "prerender": "ng run sample:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.6",
    "@angular/common": "^18.0.6",
    "@angular/compiler": "^18.0.6",
    "@angular/core": "^18.0.6",
    "@angular/fire": "../../dist/packages-dist",
    "@angular/forms": "^18.0.6",
    "@angular/platform-browser": "^18.0.6",
    "@angular/platform-browser-dynamic": "^18.0.6",
    "@angular/platform-server": "^18.0.6",
    "@angular/router": "^18.0.6",
    "@angular/service-worker": "^18.0.6",
    "@angular/ssr": "^18.0.7",
    "cross-fetch": "^3.1.4",
    "express": "^4.15.2",
    "firebase-admin": "^10.0.2",
    "lodash.isequal": "^4.5.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "webpack": "^5.35.0",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1800.7",
    "@angular-devkit/build-angular": "^18.0.7",
    "@angular/cli": "^18.0.7",
    "@angular/compiler-cli": "^18.0.6",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "ajv-formats": "^2.1.1",
    "browser-sync": "^3.0.0",
    "file-loader": "^6.2.0",
    "firebase-functions": "^3.6.0",
    "firebase-functions-test": "^0.2.2",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^18.0.0",
    "ts-mixer": "^6.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~5.4.5",
    "webpack-bundle-analyzer": "^4.4.1"
  }
}
