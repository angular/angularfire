{
  "name": "ng-build",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "dev:ssr": "ng run ng-build:serve-ssr",
    "serve:ssr": "node dist/ng-build/server/main.js",
