  "description": "Ng Deploy target options for Firebase.",
  "properties": {
    "buildTarget": {
      "type": "string",
      "description": "Target to build.",
      "pattern": "^[^:\\s]+:[^:\\s]+(:[^\\s]+)?$"
    }
  }
}
